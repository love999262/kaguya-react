{
    "compilerOptions": {
        "allowJs": true,
        "sourceMap": true,
        "declaration": false,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "allowSyntheticDefaultImports": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "jsx": "react",
        "lib": [
            "dom",
            "es5",
            "es6",
            "scripthost",
            "es2015"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}